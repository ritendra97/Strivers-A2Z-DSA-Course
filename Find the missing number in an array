package Array;

public class Findthemissingnumberinanarray {

    public int missingNumber(int[] a, int N) {
        // Check each number from 1 to N
        for (int i = 1; i <= N; i++) {
            boolean found = false;

            // Check if i exists in the array using linear search
            for (int j = 0; j < N - 1; j++) {
                if (a[j] == i) {
                    found = true;
                    break;
                }
            }

            if (!found)
                return i;
        }

        return -1;
    }

    public int missingNumberOptimize(int[] a, int N) {

        int sum = (N * (N + 1)) / 2;

        int actualSum = 0;
        for (int i = 0; i < N - 1; i++) {
            actualSum += a[i];
        }

        return sum - actualSum;
    }

    public static void main(String[] args) {

        int[] a = { 1, 2, 4, 5 };
        int N = 5;

        Findthemissingnumberinanarray obj = new Findthemissingnumberinanarray();
        int ans = obj.missingNumber(a, N);
        System.out.println("The missing number is: " + ans);
    }
}
